<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="CloseBuic Suite ">


	<listeners>
		<listener class-name="Utility.ExtentListeners" />
	</listeners>

	<test thread-count="5" parallel="methods" name="CloseBuic Test">
		<parameter name="browser" value="chrome"></parameter>
		<classes>

			<class name="Ac.zNewFormTest" />

		</classes>
	</test> <!-- Test -->


	<test thread-count="5" parallel="methods"
		name="CloseBuic Test edge">
		<parameter name="browser" value="edge"></parameter>

		<classes>

			<class name="Ac.zNewFormTest" />

		</classes>
	</test> <!-- Test -->
</suite> <!-- Suite -->
